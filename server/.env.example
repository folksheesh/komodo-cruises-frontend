# Example environment variables for server
# Copy to .env and fill values before running locally or deploying

# Resend API key (keep secret)
RESEND_API_KEY=

# Verified "from" address (must be a verified domain in Resend)
EMAIL_FROM=booking@komodocruises.com

# Server port
PORT=3001

# Xendit secret key (use production key on production environment)
XENDIT_SECRET_KEY=
